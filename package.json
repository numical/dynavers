    "test": "jasmine ./test/test.js"
  },
  "dependencies": {
    "rimraf": "^2.4.3",
    "rsvp": "^3.1.0",
    "spawn-cmd": "0.0.2"
  },
  "devDependencies": {
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "jasmine2-custom-message": "^0.8.2",
    "semistandard": "^9.0.0"
  }
}
