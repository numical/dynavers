{
  "name": "dynavers",
  "description": "Install multiple versions of a module and switch between them",
  "version": "0.2.0",
  "author": "Mike Evans <mike.evans@numical.com> (https://github.com/numical)",
  "repository": {
    "type": "git",
    "url": "https://github.com/numical/dynavers"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "install-module-versions": "bin/install-module-versions"
  },
  "engines": {
    "node": ">= 4"
  },
  "keywords": [
    "npm",
    "testing",
    "module",
    "require"
